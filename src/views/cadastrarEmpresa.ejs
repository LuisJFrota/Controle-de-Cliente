  <body class="bg-light m">
    <div class="p-5 m-5 bg-light border">
      <%for(erros of Erros){%>
        <div class="alert alert-danger">
          <%=erros%>
        </div>
      <%}%>
      <form action="/registershop" method="post">
        <div class="container">
            <div class="col py-2">
              <div class="col">
                <label for="firstName" class="form-label">Nome da loja</label>
                <input type="text" class="form-control" id="name" name="name">
              </div>
            </div>     
            <div class="row">
              <div class="col-5 py-2">
                <label for="lastName" class="form-label">CNPJ</label>
                <input type="text" class="form-control" id="cnpj" name="cnpj">
              </div>
              <div class="col py-2">
                <label for="emailInfo" class="form-label">E-mail</label>
                <input type="email" class="form-control" id="email" name="email">
              </div>
          </div>
          <div class="row">
            <div class="col-3 py-2">
              <label for="lastName" class="form-label">Tipo de Produto</label>
              <input type="cnpj" class="form-control" id="tipoProduto" name="tipoProduto">
            </div>
            <div class="col-3 py-2">
              <label for="phoneNumber" class="form-label">Telefone</label>
              <input type="text" class="form-control" id="telefone" name="telefone" placeholder="(88) 9999-9999">
            </div>
            <div class="col py-2">
              <label for="phoneNumber" class="form-label">Url da loja</label>
              <input type="text" class="form-control" id="url" name="url">
            </div>
          </div>
          <div class="row">
            <div class="col py-2">
              <label for="lastName" class="form-label">CPF do responsavel</label>
              <input type="cnpj" class="form-control" id="cpf" name="cpf">
            </div>
            <div class="col py-2">
              <label for="lastName" class="form-label">Cidade</label>
              <input type="cnpj" class="form-control" id="cidade" name="cidade">
            </div>
            <div class="col py-2">
              <label for="lastName" class="form-label">Estado</label>
              <input type="cnpj" class="form-control" id="estado" name="estado">
            </div>
          </div>    
              <div class="col-md-12 py-2">
                <button type="submit" class="btn btn-success">Salvar</button>
              </div>
          </div>
        </form>
    </div>  
  </body>
